<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox prefWidth="200.0" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.marvk.marpletraffic.app.view.control.ControlView">
    <children>
        <ToggleButton fx:id="inspectButton" mnemonicParsing="false" text="Inspect"/>
        <ToggleButton fx:id="addButton" mnemonicParsing="false" text="Add"/>
        <ToggleButton fx:id="deleteButton" mnemonicParsing="false" text="Delete"/>
       <Button onAction="#spawn" mnemonicParsing="false" text="Spawn Agents"/>
    </children>
</VBox>
